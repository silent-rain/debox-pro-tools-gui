{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "debox-pro-tools-gui",
  "version": "0.1.0",
  "identifier": "com.one.debox-pro-tools-gui",
  "build": {
    "beforeDevCommand": "cd ../mobile-web && pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cd ../mobile-web && pnpm build",
    "frontendDist": "../../mobile-web/dist"
  },
  "app": {
    "windows": [
      {
        "title": "Debox Pro Tools Gui",
        "width": 600,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "Utility",
    "android": {
      "minSdkVersion": 28
    },
    "windows": {
      "webviewInstallMode": {
        // downloadBootstrapper/embedBootstrapper/offlineInstaller
        "type": "embedBootstrapper"
      }
    },
    "licenseFile": "../../LICENSE"
  }
}